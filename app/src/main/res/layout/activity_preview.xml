<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.tflitecamerademo.PreviewActivity"
    android:background="@drawable/start_bk">

    <ImageView
        android:id="@+id/imageView_static"
        android:layout_width="wrap_content"
        android:layout_height="435dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="17dp"
        android:rotation="90"
        android:src="@color/control_background" />

    <TextView
        android:id="@+id/textView_static"
        android:layout_width="wrap_content"
        android:layout_height="93dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="24dp" />

    <Button
        android:id="@+id/preview_back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_above="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="-40dp"
        android:layout_marginBottom="-45dp"
        android:layout_toStartOf="@+id/imageView"
        android:background="@drawable/back"
        android:layout_marginLeft="10dp"/>
</RelativeLayout>
